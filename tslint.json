{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "max-classes-per-file": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "max-line-length": false,
    "no-namespace": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
